    <image class="setting__avatar" src="/icon_png/ic_chatbot_h.png" mode="cover" />
  </navigator>

  <button class="setting" open-type="getUserInfo" bindgetuserinfo="getUserInfo" wx:else>
    <image class="setting__avatar" src="/icon_png/ic_chatbot_n.png" mode="cover" />
  </button>
</block>
